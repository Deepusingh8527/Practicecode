#include<iostream>
using namespace std;

int main(){

  string str = "my name is raju sharma";
  int n  = str.length();

  int i = 0;
  while(i < n){
    if(i == 0 || i == 3 || i == 8 || i == 11 || i == 16){
      
  if(str[i] >= 'a' && str[i] <= 'z')
    str[i] = str[i] - 'a' + 'A';


  }
        i++;
  }
  cout << str;
  return 0;
}
