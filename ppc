// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int sum;
void natural(int number){
    
    for(int i = 1; i <= number; i++){
    sum = sum + i;
    }
    
  cout << sum;
    
    
}

int main() {
    // Write C++ code here
int number;
cout << "Enter the any Natural Number :: ";
cin >> number;

 natural(number);

int arr[20] = {2,3,4,5,6,2,4,5,0,98,7,6,5,3};

int size = sizeof(arr)/sizeof(arr[0]);

cout << size <<endl;

for(int i = 0; i < size; i++){
    cout<<arr[i]<<" ";
}


    return 0;
}
