#include<iostream>
#include<vector>
using namespace std;

int main()
{
  string s = "my name is deepu";
  int ans = 0;

  int n = s.length();
  
  for(int i = 0; i < n; i++){
    
    while(i < n && s[i] != ' '){
      ans++;
      i++;
   
    }
     if(s[i] == ' '){
        cout<<ans<<" ";
       ans = 0;
     }
  
  if(ans > 0){
    cout<< ans <<endl;
  }
  }

  

  return 0;
}
